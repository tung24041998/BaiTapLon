@model List<BaiTapLon.EF.AlBum>
@{
    ViewBag.Title = "AlBum";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br />
<div class="row">

    @foreach (var item in Model)
    {
        <div class="col-sm-6 col-md-4">
            <div class="thumbnail">
                <img src="@item.Anh" alt="">
                <div class="caption">
                    <h3>@item.TenAlBum</h3>
                    <p><a href="#" class="btn btn-primary" role="button">Album</a></p>
                </div>
            </div>
        </div>
    }

</div>
<br />


